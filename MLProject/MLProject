name: Brain_Tumor_CI_Project

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      data_path: {type: str, default: "dataset_preprocessing/brain_data.pkl"}
      epochs: {type: int, default: 3}
      batch_size: {type: int, default: 16}
    command: "python modelling.py --data_path {data_path} --epochs {epochs} --batch_size {batch_size}"
